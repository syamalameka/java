class Constructor {
    String name;
    int id;
    int age;

    Constructor(int i, String n) {
        id = i;
        name = n;
    }

    Constructor(int i, String n, int a) {
        id = i;
        name = n;
        age = a;
    }

    void display() {
        System.out.println(id + " " + name + " " + age);
    }
}

 public class Main {
    public static void main(String[] args) {

        Constructor s1 = new Constructor(111, "Kiran");
        Constructor s2 = new Constructor(222, "Arya", 25);

        s1.display();
        s2.display();
    }
}
