public class Main {
    public static void main(String[] args) {

        int[] numbers = {10, 20, 30, 40, 50};
        int removeIndex = 2;

        int[] newArray = new int[numbers.length - 1];

        for (int i = 0, j = 0; i < numbers.length; i++) {
            if (i != removeIndex) {
                newArray[j++] = numbers[i];
            }
        }

        for (int n : newArray) {
            System.out.print(n + " ");
        }
    }
}
