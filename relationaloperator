public class Operators {
public static void main(String[] args) {
int a = 5;
int b = 10;
int c = 15;
System.out.println("a > b: " + (a>b));
System.out.println("a < b: " + (a<b));
System.out.println("a <= b: " + (a<=b));
System.out.println("a >= b: " + (a>=b));
System.out.println("a == c: " + (a==c));
System.out.println("a !=c: " + (a!=b));
}
}
